    isbn = {1471-2105},
    issn = {1471-2105},
    journal = {BMC bioinformatics},
    keywords = {Chromatography, Liquid,Chromatography, Liquid: methods,Databases, Factual,Mass Spectrometry,Mass Spectrometry: methods,Models, Statistical},
    number = {1},
    pages = {559},
    pmid = {21073736},
    title = {{Quantification and deconvolution of asymmetric LC-MS peaks using the bi-Gaussian mixture model and statistical model selection.}},
    url = {http://www.biomedcentral.com/1471-2105/11/559},
    volume = {11},
